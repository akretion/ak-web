         <!-- Create a service product  adaptoo Colissimo International  Zone 5 -->
            <!-- max 30 kg,-->

        <record id="product_product_delivery_inter_zone5" model="product.product">
            <field name="name">Colissimo International Zone 5</field>
            <field name="default_code">colissimo_5_30</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_6"/>
            <field name="standard_price"></field>
            <field name="list_price"></field>
            <field name="image" type="base64" file="delivery/static/img/product_product_delivery-image.jpg"/>
        </record>

