        <record id="delivery_grid_inter_zone5" model="delivery.grid">
            <field name="name">Pricing Colissimo International Zone 5</field>
            <field name="carrier_id" ref="delivery_carrier_inter_zone5"/>
            <field model="res.country" name="country_ids" search="[('code','ilike','AT')]"/>
        </record>

