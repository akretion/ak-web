        <record id="delivery_carrier_inter_zone5" model="delivery.carrier">
            <field name="name">Colissimo International Zone 5</field>
            <field name="normal_price"></field>
            <field name="partner_id" ref="res_partner_74"/>
            <field name="product_id" ref="product_product_delivery_inter_zone5"/>
            <field name="use_detailed_pricelist" eval="True"/>
        </record>

