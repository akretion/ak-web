    <!-- ######################################################################################################  -->

         <!-- Create a service product  adaptoo Colissimo International zone 3 -->
            <!-- max 30 kg,-->

        <record id="product_product_delivery_inter_zone3" model="product.product">
            <field name="name">Colissimo International Zone 3</field>
            <field name="default_code">colissimo_3_30</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_6"/>
            <field name="standard_price"></field>
            <field name="list_price"></field>
            <field name="image" type="base64" file="delivery/static/img/product_product_delivery-image.jpg"/>
        </record>

         <!-- Carrier -->
        <record id="delivery_carrier_inter_zone3" model="delivery.carrier">
            <field name="name">Colissimo International Zone 3</field>
            <field name="normal_price"></field>
            <field name="partner_id" ref="res_partner_74"/>
            <field name="product_id" ref="product_product_delivery_inter_zone3"/>
            <field name="use_detailed_pricelist" eval="True"/>
        </record>

        <!-- Carrier Grids -->
        <record id="delivery_grid_inter_zone3" model="delivery.grid">
            <field name="name">Pricing Colissimo International Zone 3</field>
            <field name="carrier_id" ref="delivery_carrier_inter_zone3"/>
            <field model="res.country" name="country_ids" search="[('code','ilike','AT')]"/>
        </record>

